<h1>書名一覧</h1>

<ul>
  <% @books.each do |message| %>
    <li><%= link_to message.id, message %></li>
    <li><%= message.book_name %></li>
  <% end %>
</ul>

<h1>著者一覧</h1>
<ul>
  <% @authors.each do |message| %>
  <li><%= link_to message.id, message %>
    <li><%= message.author_name %></li>
  <% end %>
</ul>
